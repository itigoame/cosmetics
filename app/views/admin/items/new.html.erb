<div class="container">

<%= form_with model: @item,url: admin_items_path do |f| %>
<div>
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>
<div>
  <%= f.label :body %>
  <%= f.text_area  :body %>
</div>
<div>
  <%= f.label :price %>
  <%= f.text_field :price %>
</div>
<div>
  <%#= f.label :image %>
  <%#= f.file_field :image, accept: "image/*" %>
</div>


  <%= f.submit %>
<% end %>

</div>